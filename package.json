{
  "name": "n8n-nodes-autotask",
  "version": "2.1.0",
  "description": "n8n node for Autotask PSA integration",
  "keywords": [
    "n8n-community-node-package",
    "autotask",
    "psa",
    "datto",
    "n8n"
  ],
  "license": "MIT",
  "homepage": "https://github.com/msoukhomlinov/n8n-nodes-autotask",
  "author": {
    "name": "Max Soukhomlinov",
    "email": "maxs@intellectit.com.au"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msoukhomlinov/n8n-nodes-autotask.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "dist/nodes/Autotask/Autotask.node.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm clean && tsc && gulp build:icons",
    "dev": "gulp build:icons && tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AutotaskApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Autotask/Autotask.node.js",
      "dist/nodes/Autotask/AutotaskTrigger.node.js",
      "dist/nodes/Autotask/AutotaskAiTools.node.js"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^20.11.17",
    "@types/pluralize": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "globals": "^14.0.0",
    "gulp": "^5.0.0",
    "n8n-workflow": "^1.70.0",
    "prettier": "^3.3.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@langchain/core": "^0.3.43",
    "change-case": "^4.1.2",
    "keyv": "^5.6.0",
    "keyv-file": "^5.3.3",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.47",
    "pluralize": "^8.0.0",
    "zod": "^3.23.0"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  }
}
